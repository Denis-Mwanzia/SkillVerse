{
  "name": "Skillverse",
  "version": "1.0.0",
  "description": "Jac application: Skillverse",
  "main": "index.js",
  "scripts": {
    "build": "npm run compile && vite build",
    "dev": "vite dev",
    "preview": "vite preview",
    "compile": "babel src --out-dir build --extensions \".jsx,.js\" --out-file-extension .js && node -e \"const fs=require('fs'); const f='build/main.js'; let c=fs.readFileSync(f,'utf8'); if(!c.includes('index.css')){c=c.replace('import { app as App } from \\\"./app.js\\\";','import { app as App } from \\\"./app.js\\\";\\nimport \\\"../src/index.css\\\";'); fs.writeFileSync(f,c);}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@tailwindcss/vite": "^4.0.0",
    "tailwindcss": "^4.0.0",
    "vite": "^6.4.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^6.30.1"
  }
}
